{
  "source": ["tokens/**/*.json", "tokens/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "build/scss/",
      "files": [{
        "destination": "_variables.scss",
        "format": "scss/variables"
      }]
    },
    "css": {
      "transformGroup": "tokens-studio",
      "prefix":"sd",
      "buildPath": "build/css/",
      "files": [
        {
          "destination": "_variables-%theme%.css",
          "format": "css/variables"
        }]
    }
  }
}
